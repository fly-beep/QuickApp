{
  "package": "com.edu.hit.todos",
  "name": "SmartTodo",
  "versionName": "1.0.0",
  "versionCode": 1,
  "icon": "/Common/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "service.asr"
    },
    {
      "name": "system.vibrator"
    },
    {
      "name": "system.storage"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {
    "logLevel": "log"
  },
  "router": {
    "entry": "MainPage",
    "pages": {
      "MainPage": {
        "component": "index"
      },
      "Input": {
        "component": "index"
      },
      "Draw": {
        "component": "index"
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": false,
    "menuBarData": {
      "menuBar": true
    }
  }
}